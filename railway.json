{
  "name": "my-dev-env",
  "services": [
    {
      "name": "app",
      "startCommand": "pnpm start",
      "buildCommand": "pnpm build",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000",
        "DATABASE_URL": "postgresql://postgres:postgres@db:5432/mydb?schema=public"
      }
    },
    {
      "name": "db",
      "type": "postgres",
      "version": "16"
    }
  ]
}
